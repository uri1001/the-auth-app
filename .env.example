# server

# server port number
PORT=<server-port-number> # default value - 3000

# jwt

# user session length
JWT_SESSION_LENGTH_SECONDS=<jwt-session-length-in-seconds> # default value - 1800

# local jwt public-private keys
JWT_PUBLIC_KEY=<jwt-local-public-key> # default value - -----BEGIN PUBLIC KEY----------END PUBLIC KEY-----
JWT_PRIVATE_KEY=<jwt-local-private-key> # default value - -----BEGIN ENCRYPTED PRIVATE KEY----------END ENCRYPTED PRIVATE KEY-----

# db

# local database encryption symmetric key - AES-256
DB_KEY=<db-encryption-key> # default value - IZkik1ZmpR5G/thTetA8LL8pwTPRpqJLtkptxey4MZ4=
DB_IV=<db-encryption-initialization-vector> # default value - RVeiwFBCBYBo7NQ0q/d5dw==

# radius

# radius server secret
RADIUS_SERVER_SECRET=<radius-server-secret> # default value - 0

# oauth

# requires authentication project in github
# https://github.com/ -> Developer Settings -> OAuth Apps
OAUTH2_CLIENT_ID=<oauth2-client-id> # default value - 0
OAUTH2_CLIENT_SECRET=<oauth2-client-secret> # default value - 0

# authetication github service
OAUTH2_AUTHORIZE_URL=<oauth2-authorize-url> # default value - https://github.com/login/oauth/authorize
# authentication token service
OAUTH2_TOKEN_URL=<oauth2-token-url> # default value - https://github.com/login/oauth/access_token
# user api service
OAUTH2_USER_API=<user-api> # default value - https://api.github.com/user

# openid

# requires authentication project in google - https://console.cloud.google.com/
OIDC_CLIENT_ID=<oidc-client-id> # default value - 0
OIDC_CLIENT_SECRET=<oidc-client-secret> # default value - 0

# authetication google service
OIDC_PROVIDER=<oidc-provider> # default value - https://accounts.google.com
# authentication local enviroment
OIDC_CALLBACK_URL=<oidc-callback-url> # default value - http://localhost:3000/oidc/cb

# vc

# verifiable credentials provider resources - request provider
VC_PROVIDER_ISSUE_URL="https://staging.werify.eu/#/<vc-endpoint>"
VC_PROVIDER_LOGIN_URL="https://staging.werify.eu/#/<vc-endpoint>"

# verifiable credentials provider endpoints authentication - request provider
VC_PROVIDER_ENDPOINT_AUTH_ID=<endpoint-authentication-id>
VC_PROVIDER_ENDPOINT_AUTH_PWD=<endpoint-authentication-password>

# verifiable credentials provider jwt public key - request provider
VC_PROVIDER_PUBLIC_KEY=<jwt-provider-public-key>